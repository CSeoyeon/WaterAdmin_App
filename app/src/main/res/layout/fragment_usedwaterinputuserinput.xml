<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/usedWaterInputUserInput_tv_title"

        android:layout_width="205dp"
        android:layout_height="51dp"
        android:text="사용자 직접 입력"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="38dp" />

    <TextView
        android:id="@+id/usedWaterInputUserInput_tv_subtitle"
        android:layout_width="146dp"
        android:layout_height="45dp"
        android:text="리터(L) 기준 "
        android:textSize="20sp"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="89dp" />

    <EditText
        android:id="@+id/usedWaterInputUserInput_et_usedCount"
        android:layout_width="0dp"
        android:layout_height="43dp"

        android:ems="10"
        android:inputType="number"
        android:text="1"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="134dp" />

    <TextView
        android:id="@+id/usedWaterInputUserInput_tv_countTitle"
        android:layout_width="146dp"
        android:layout_height="45dp"
        android:text="L"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="257dp"
        tools:layout_editor_absoluteY="134dp" />

</androidx.constraintlayout.widget.ConstraintLayout>